{
  "name": "angular-dynamic-routes",
  "version": "1.0.1",
  "author": {
    "email": "adem@simsek.dev",
    "name": "Adem Simsek"
  },
  "scripts": {
    "main": "ng-packagr -p projects/main/package.json -c projects/main/tsconfig.json",
    "shared": "ng-packagr -p projects/shared/package.json -c projects/shared/tsconfig.json",
    "serve": "live-server demo --mount=/dist:./dist --mount=/node_modules:./node_modules --entry-file=index.html",
    "postinstall": "ngcc --properties es2015"
  },
  "browserslist": [
    "Chrome > 89"
  ],
  "private": true,
  "dependencies": {
    "@angular/common": "~12.2.6",
    "@angular/compiler": "~12.2.6",
    "@angular/core": "~12.2.6",
    "@angular/platform-browser": "~12.2.6",
    "@angular/router": "~12.2.6",
    "tslib": "^2.0.1"
  },
  "devDependencies": {
    "@angular/cli": "~12.2.6",
    "@angular/compiler-cli": "~12.2.6",
    "live-server": "^1.2.1",
    "ng-packagr": "~16.2.3",
    "typescript": "~4.3.5"
  }
}
